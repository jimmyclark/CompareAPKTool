<?xml version="1.0" encoding="UTF-8"?>
<Dialog
    childContent="Layout"
    content="FailResultDialog"
    height="fill_parent"
    width="fill_parent" >

    <Item
        background="mmiap/03/bg.png"
        content="scrollview"
        height="fill_parent"
        orientation="vertical"
        stytle="ScrollView"
        width="wrap_content" >

        <Item
            background="mmiap/03/bg.png"
            content="PurchaseLayout"
            height="fill_parent"
            orientation="vertical"
            stytle="LinearLayout"
            width="fill_parent" >

            <Item
                background="mmiap/03/title1_bg.png"
                content="MainTiteLayout"
                height="70"
                stytle="RelativeLayout"
                width="fill_parent" >

                <Item
                    background="mmiap/03/logo1.png"
                    content="MainTitle"
                    gravity="center_in_parent"
                    height="70"
                    stytle="TextView"
                    width="200" >
                </Item>
            </Item>

            <Item
                background="mmiap/03/title2_bg.png"
                content="smallTitlelayout"
                height="60"
                paddingLeft="10"
                paddingRight="20"
                stytle="RelativeLayout"
                width="fill_parent" >

                <Item
                    content="SMALLTitle"
                    gravity="center_in_parent"
                    height="wrap_content"
                    paddingLeft="5"
                    stytle="TextView"
                    text="支 付 中 心"
                    textsize="20"
                    width="wrap_content" >
                </Item>

                <Item
                    background="mmiap/03/button_back_press.png"
                    clickable="true"
                    content="backbutton"
                    gravity="align_parent_left|r_center_vertical"
                    height="40"
                    pressedBG="mmiap/03/button_back.png"
                    stytle="ImageView"
                    width="60" >
                </Item>
            </Item>

            <Item
                content="ResultLayout"
                gravity="center_horizontal"
                height="wrap_content"
                orientation="vertical"
                paddingBottom="10"
                paddingLeft="10"
                paddingRight="10"
                paddingTop="15"
                stytle="LinearLayout"
                width="fill_parent" >

                <Item
                    background="mmiap/03/infobg.9.png"
                    content="ResultLayout"
                    gravity="center_horizontal"
                    height="wrap_content"
                    orientation="vertical"
                    paddingBottom="15"
                    stytle="LinearLayout"
                    width="fill_parent" >

                    <Item
                        content="ProductView"
                        gravity="center"
                        height="wrap_content"
                        margingBottom="50"
                        orientation="horizontal"
                        paddingTop="50"
                        stytle="LinearLayout"
                        width="wrap_content" >

                        <Item
                            background="mmiap/03/icon_info.png"
                            content="warnicon"
                            height="76"
                            margingRight="10"
                            stytle="TextView"
                            width="76" >
                        </Item>

                        <Item
                            content="result"
                            gravity="left"
                            height="wrap_content"
                            paddingLeft="10"
                            paddingRight="10"
                            stytle="TextView"
                            text="支 付  失  败"
                            textcolor="#ffec9200"
                            textsize="30"
                            width="wrap_content" >
                        </Item>
                    </Item>

                    <Item
                        background="mmiap/03/line.png"
                        content="line"
                        height="2"
                        margingRight="10"
                        stytle="TextView"
                        width="380" >
                    </Item>

                    <Item
                        content="ResultText"
                        gravity="center_horizontal"
                        height="wrap_content"
                        orientation="vertical"
                        paddingBottom="20"
                        paddingLeft="10"
                        paddingRight="10"
                        stytle="LinearLayout"
                        width="fill_parent" >

                        <Item
                            content="ResultMSG"
                            gravity="center"
                            height="wrap_content"
                            stytle="TextView"
                            text="尊敬的用户，商品购买失败"
                            textcolor="#ff818181"
                            textsize="15"
                            width="wrap_content" >
                        </Item>
                    </Item>
                </Item>

                <Item
                    content="buttonLayout"
                    gravity="center"
                    height="wrap_content"
                    paddingBottom="15"
                    paddingTop="15"
                    stytle="LinearLayout"
                    width="fill_parent" >

                    <Item
                        background="mmiap/03/button1_confirm.9.png"
                        content="confirmButton"
                        gravity="center"
                        height="70"
                        marginTop="20"
                        paddingBottom="10"
                        paddingTop="10"
                        pressedBG="mmiap/03/button1_confirm_press.9.png"
                        stytle="Button"
                        text="确  认"
                        textsize="25"
                        width="fill_parent" >
                    </Item>
                </Item>
            </Item>

            <Item
                content="BottomLayout"
                height="fill_parent"
                margingBottom="5"
                margingTop="50"
                orientation="horizontal"
                paddingBottom="10"
                paddingTop="10"
                stytle="RelativeLayout"
                width="fill_parent" >

                <Item
                    content="bottomContent"
                    gravity="align_parent_bottom|r_center_horizontal"
                    height="wrap_content"
                    orientation="horizontal"
                    stytle="LinearLayout"
                    width="wrap_content" >

                    <Item
                        content="logoText"
                        height="wrap_content"
                        margingBottom="5"
                        paddingLeft="3"
                        stytle="TextView"
                        text="版权所有 中国移动"
                        textsize="15"
                        width="wrap_content" >
                    </Item>

                    <Item
                        content="version"
                        height="wrap_content"
                        margingBottom="0"
                        margingBottom="5"
                        margingLeft="5"
                        paddingLeft="3"
                        stytle="TextView"
                        text="V3.1.9"
                        textcolor="#ff818181"
                        textsize="12"
                        width="wrap_content" >
                    </Item>
                </Item>
            </Item>
        </Item>
    </Item>

</Dialog>